{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "env": {
        "EXPO_PUBLIC_DOMAIN": "YOUR_REPLIT_DEV_URL:5000"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_DOMAIN": "YOUR_PRODUCTION_URL"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_DOMAIN": "YOUR_PRODUCTION_URL"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
